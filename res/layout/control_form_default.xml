<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/skin_default"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:gravity="center_vertical|center_horizontal"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/cf_dummy_up"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:editable="false"
        android:gravity="center_horizontal"
        android:singleLine="true"
        android:text="" >
    </TextView>

    <TextView
        android:id="@+id/cf_title"
        style="?android:attr/textAppearanceMedium"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:editable="false"
        android:ellipsize="marquee"
        android:gravity="center_horizontal"
        android:singleLine="true" >
    </TextView>

    <!--
        TextView android:layout_width="fill_parent" android:id="@+id/cf_title_small" 
		android:layout_height="wrap_content" android:editable="false" android:gravity="center_horizontal" 
		android:singleLine="true" android:visibility="gone" style="?android:attr/textAppearanceSmall" 
		android:ellipsize="marquee"></TextView> <TextView android:layout_width="fill_parent" 
		android:id="@+id/cf_title_large" android:layout_height="wrap_content" android:editable="false" 
		android:gravity="center_horizontal" android:singleLine="true" android:visibility="gone" 
		style="?android:attr/textAppearanceLarge" android:ellipsize="marquee"></TextView
    -->

    <TableLayout
        android:id="@+id/icons"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical|center_horizontal" >

        <TableRow
            android:id="@+id/row1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:orientation="horizontal" >

            <TableLayout android:id="@+id/tl_b1" >
               <TableRow android:id="@+id/tr_b1" >
                  <ImageButton
                        android:id="@+id/b1"
                        android:adjustViewBounds="true"
                        android:background="@drawable/btn_selector"
                        android:nextFocusDown="@+id/b4"
                        android:nextFocusRight="@+id/b2"
                        android:scaleType="centerInside" >
                    </ImageButton>
                </TableRow>
            </TableLayout>

            <TableLayout android:id="@+id/tl_b2" >
               <TableRow android:id="@+id/tr_b2" >
                   <ImageButton
                        android:id="@+id/b2"
                        android:adjustViewBounds="true"
                        android:background="@drawable/btn_selector"
                        android:nextFocusDown="@+id/b5"
                        android:nextFocusLeft="@+id/b1"
                        android:nextFocusRight="@+id/b3"
                        android:scaleType="centerInside" >
                    </ImageButton>
                </TableRow>
            </TableLayout>

            <TableLayout android:id="@+id/tl_b3" >
                <TableRow android:id="@+id/tr_b3" >
                    <ImageButton
                        android:id="@+id/b3"
                        android:adjustViewBounds="true"
                        android:background="@drawable/btn_selector"
                        android:nextFocusDown="@+id/b6"
                        android:nextFocusLeft="@+id/b2"
                        android:scaleType="centerInside" >
                    </ImageButton>
                </TableRow>
            </TableLayout>
        </TableRow>

        <TableRow
            android:id="@+id/row2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical|center_horizontal"
            android:orientation="horizontal" >

            <TableLayout android:id="@+id/tl_b4" >
                <TableRow android:id="@+id/tr_b4" >
                    <ImageButton
                        android:id="@+id/b4"
                        android:adjustViewBounds="true"
                        android:background="@drawable/btn_selector"
                        android:nextFocusDown="@+id/b7"
                        android:nextFocusRight="@+id/b5"
                        android:nextFocusUp="@+id/b1"
                        android:scaleType="centerInside" >
                    </ImageButton>
                </TableRow>
            </TableLayout>

            <TableLayout android:id="@+id/tl_b5" >
                <TableRow android:id="@+id/tr_b5" >
                    <ImageButton
                        android:id="@+id/b5"
                        android:adjustViewBounds="true"
                        android:background="@drawable/btn_selector"
                        android:nextFocusDown="@+id/b8"
                        android:nextFocusLeft="@+id/b4"
                        android:nextFocusRight="@+id/b6"
                        android:nextFocusUp="@+id/b2"
                        android:scaleType="centerInside" >
                    </ImageButton>
                </TableRow>
            </TableLayout>

            <TableLayout android:id="@+id/tl_b6" >
                <TableRow android:id="@+id/tr_b6" >
                    <ImageButton
                        android:id="@+id/b6"
                        android:adjustViewBounds="true"
                        android:background="@drawable/btn_selector"
                        android:nextFocusDown="@+id/b9"
                        android:nextFocusLeft="@+id/b5"
                        android:nextFocusUp="@+id/b3"
                        android:scaleType="centerInside" >
                    </ImageButton>
                </TableRow>
            </TableLayout>
        </TableRow>

        <TableRow
            android:id="@+id/row3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:orientation="horizontal" >

            <TableLayout android:id="@+id/tl_b7" >
                <TableRow android:id="@+id/tr_b7" >
                    <ImageButton
                        android:id="@+id/b7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:adjustViewBounds="true"
                        android:background="@drawable/btn_selector"
                        android:nextFocusDown="@+id/b10"
                        android:nextFocusRight="@+id/b8"
                        android:nextFocusUp="@+id/b4"
                        android:scaleType="centerInside" >
                    </ImageButton>
                </TableRow>
            </TableLayout>

            <TableLayout android:id="@+id/tl_b8" >
                <TableRow android:id="@+id/tr_b8" >
                    <ImageButton
                        android:id="@+id/b8"
                        android:adjustViewBounds="true"
                        android:background="@drawable/btn_selector"
                        android:nextFocusDown="@+id/b0"
                        android:nextFocusLeft="@+id/b7"
                        android:nextFocusRight="@+id/b9"
                        android:nextFocusUp="@+id/b5"
                        android:scaleType="centerInside" >
                    </ImageButton>
                </TableRow>
            </TableLayout>

            <TableLayout android:id="@+id/tl_b9" >
                <TableRow android:id="@+id/tr_b9" >
                    <ImageButton
                        android:id="@+id/b9"
                        android:adjustViewBounds="true"
                        android:background="@drawable/btn_selector"
                        android:nextFocusDown="@+id/b11"
                        android:nextFocusLeft="@+id/b8"
                        android:nextFocusUp="@+id/b6"
                        android:scaleType="centerInside" >
                    </ImageButton>
                </TableRow>
            </TableLayout>
        </TableRow>

        <TableRow
            android:id="@+id/row4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:orientation="horizontal" >

            <TableLayout android:id="@+id/tl_b10" >
                <TableRow android:id="@+id/tr_b10" >
                    <ImageButton
                        android:id="@+id/b10"
                        android:adjustViewBounds="true"
                        android:background="@drawable/btn_selector"
                        android:nextFocusRight="@+id/b0"
                        android:nextFocusUp="@+id/b7"
                        android:scaleType="centerInside" >
                    </ImageButton>
                </TableRow>
            </TableLayout>

            <TableLayout android:id="@+id/tl_b11" >
                <TableRow android:id="@+id/tr_b11" >
                   <ImageButton
                        android:id="@+id/b0"
                        android:adjustViewBounds="true"
                        android:background="@drawable/btn_selector"
                        android:nextFocusLeft="@+id/b10"
                        android:nextFocusRight="@+id/b11"
                        android:nextFocusUp="@+id/b8"
                        android:scaleType="centerInside" >
                    </ImageButton>
                </TableRow>
            </TableLayout>

            <TableLayout android:id="@+id/tl_b12" >
                <TableRow android:id="@+id/tr_b12" >
                    <ImageButton
                        android:id="@+id/b11"
                        android:adjustViewBounds="true"
                        android:background="@drawable/btn_selector"
                        android:nextFocusLeft="@+id/b0"
                        android:nextFocusUp="@+id/b9"
                        android:scaleType="centerInside" >
                    </ImageButton>
                </TableRow>
            </TableLayout>
        </TableRow>

        <!--
            TableRow android:id="@+id/row5" android:layout_height="wrap_content" 
			android:orientation="horizontal" android:gravity="center_horizontal" android:layout_width="fill_parent"> 
			<SeekBar android:id="@+id/volume_bar" android:layout_width="fill_parent" 
			android:layout_height="wrap_content" android:layout_span="3" android:clickable="false" 
			android:longClickable="false" android:focusable="false" android:focusableInTouchMode="false" 
			android:enabled="false"> </SeekBar> </TableRow
        -->

    </TableLayout>

    <TextView
        android:id="@+id/cf_status"
        style="?android:attr/textAppearanceMedium"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:ellipsize="marquee"
        android:gravity="center_horizontal" >
    </TextView>

    <TextView
        android:id="@+id/cf_dummy_dn"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:editable="false"
        android:gravity="center_horizontal"
        android:singleLine="true"
        android:text="" >
    </TextView>

</LinearLayout>